<function-unit name="add">
    <port name="P1">
        <connects-to>S1</connects-to>
        <width>32</width>
        <triggers/>
        <sets-opcode/>
    </port>
    <port name="P2">
        <connects-to>S2</connects-to>
        <width>32</width>
    </port>
    <port name="P3">
        <connects-to>S2</connects-to>
        <connects-to>S3</connects-to>
        <width>32</width>
    </port>
    <operation>
        <name>operation1</name>
        <bind name="1">P1</bind>
        <pipeline>
            <resource name="res1">
                <start-cycle>0</start-cycle>
                <cycles>5</cycles>
            </resource>
            <reads name="1">
                <start-cycle>0</start-cycle>
                <cycles>5</cycles>
            </reads>
        </pipeline>
    </operation> 
    <address-space>AS1</address-space>
</function-unit>

<function-unit name="add_complex">
    <port name="P4">
        <connects-to>S4</connects-to>
        <connects-to>S7</connects-to>
        <width>32</width>
    </port>
    <port name="P5">
        <connects-to>S5</connects-to>
        <width>32</width>
        <triggers/>
        <sets-opcode/>
    </port>
    <port name="P6">
        <connects-to>S6</connects-to>
        <width>32</width>
    </port>
    <port name="P7">
        <connects-to>S6</connects-to>
        <width>32</width>
    </port>
    <port name="P8">
        <connects-to>S6</connects-to>
        <connects-to>S7</connects-to>
        <width>32</width>
        <no-register/>
    </port>
    <port name="P9">
        <connects-to>S8</connects-to>
        <width>32</width>
    </port>
    <operation>
        <name>operation1</name>
        <bind name="1">P5</bind>
        <pipeline>
            <resource name="res1">
                <start-cycle>0</start-cycle>
                <cycles>5</cycles>
            </resource>
            <reads name="1">
                <start-cycle>0</start-cycle>
                <cycles>5</cycles>
            </reads>
        </pipeline>
    </operation>
    <address-space>AS1</address-space>
</function-unit>

<register-file name="integer">
    <type>normal</type>
    <size>8</size>
    <width>32</width>
    <max-reads>1</max-reads>
    <max-writes>1</max-writes>
    <port name="P10">
        <connects-to>S9</connects-to>
    </port>
    <port name="P11">
        <connects-to>S10</connects-to>
    </port>
    <port name="P12">
        <connects-to>S8</connects-to>
    </port>
</register-file>

<address-space name="AS1">
    <width>32</width>
    <min-address>1</min-address>
    <max-address>32</max-address>
</address-space>

<address-space name="AS2">
    <width>32</width>
    <min-address>1</min-address>
    <max-address>32</max-address>
</address-space>

<global-control-unit name="gcu">
    <port name="control">
        <connects-to>S12</connects-to>
        <width>32</width>
    </port>
    <special-port name="ra">
        <connects-to>S11</connects-to>
        <connects-to>S13</connects-to>
        <width>32</width>
    </special-port> 
    <return-address>ra</return-address>
    <ctrl-operation>
        <name>jump</name>
        <pipeline>
            <resource name="res1">
                <start-cycle>0</start-cycle>
                <cycles>3</cycles>
            </resource>
        </pipeline>
    </ctrl-operation>
    <address-space>AS1</address-space>
    <delay-slots>0</delay-slots>
    <guard-latency>1</guard-latency>
</global-control-unit>

<immediate-unit name="imm">
    <type>normal</type>
    <size>4</size>
    <width>32</width>
    <max-reads>1</max-reads>
    <max-writes>1</max-writes>
    <extension>zero</extension>
    <port name="P13">
        <connects-to>S11</connects-to>
    </port>
    <template name="temp">
        <slot>
            <name>B1</name>
            <width>32</width>
        </slot>
    </template>
</immediate-unit>

</adf>
