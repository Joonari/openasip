<?xml version="1.0" encoding="ISO-8859-1"?>

<adf version="1.5">

<always-write-back-results/>

<bus name="B1">
    <width>32</width>
    <guard>
        <simple-expr>
            <bool>
                <name>integer</name>
                <index>1</index>
            </bool>
        </simple-expr>
    </guard>
    <guard>
        <inverted-expr>
            <unit>
                <name>add</name>
                <port>P1</port>
            </unit>
        </inverted-expr>
    </guard>
    <segment name="seg1">
        <writes-to>seg2</writes-to>
    </segment>
    <segment name="seg2">
        <writes-to>seg3</writes-to>
    </segment>
    <segment name="seg3">
        <writes-to></writes-to>
    </segment>
    <short-immediate>
        <extension>sign</extension>
        <width>8</width>
    </short-immediate>
</bus>

<bus name="B2">
    <width>32</width>
    <segment name="seg1">
